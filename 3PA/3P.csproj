    <PostBuildEvent>echo [INFO] End of build for 3P
set NPPFOLDERNAME=3P_notepad++
if "$(PlatformName)" == "x64" (
	set NPPFOLDERNAME=3P_notepad++x64
)

if exist "$(SolutionDir)..\3P_notepad++\plugins\" ( 
	echo [INFO] Copying output files to %25NPPFOLDERNAME%25 folder...
	if exist "$(ProjectDir)bin\$(PlatformName)\Debug\$(TargetName)$(TargetExt)" (
		copy /Y "$(ProjectDir)bin\$(PlatformName)\Debug\$(TargetName)$(TargetExt)" "$(SolutionDir)..\%25NPPFOLDERNAME%25\plugins\$(TargetName)$(TargetExt)" &gt;nul
	)
	if exist "$(ProjectDir)bin\$(PlatformName)\Debug\$(TargetName).pdb" (
		copy /Y "$(ProjectDir)bin\$(PlatformName)\Debug\$(TargetName).pdb" "$(SolutionDir)..\%25NPPFOLDERNAME%25\plugins\$(TargetName).pdb") &gt;nul
	)
)

WHERE 7z &gt;nul 2&gt;nul
IF not "%25ERRORLEVEL%25" == "0" (
	echo 7z.exe not found in the path, can't create the .zip releases
	goto :label_end
) 

set RELEASENAME=3P.zip
if "$(PlatformName)" == "x64" (
	set RELEASENAME=3P_x64.zip
)

echo [INFO] Zipping files into a release named %25RELEASENAME%25...

md "$(SolutionDir)\Releases" &gt;nul 2&gt;nul
del /Q "$(SolutionDir)\Releases\%25RELEASENAME%25" &gt;nul 2&gt;nul
7z a "$(SolutionDir)\Releases\%25RELEASENAME%25" "$(ProjectDir)bin\$(PlatformName)\Debug\$(TargetName)$(TargetExt)" &gt;nul
7z a "$(SolutionDir)\Releases\%25RELEASENAME%25" "$(ProjectDir)bin\$(PlatformName)\Debug\$(TargetName).pdb" &gt;nul


:label_end
exit 0</PostBuildEvent>
